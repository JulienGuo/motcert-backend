  "paths": {
    "/certificate": {
      "post": {
        "tags": [
          "certificate"
        ],
        "consumes": [
          "application/json",
          "application/xml"
        ],
        "produces": [
          "application/xml",
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Certificate object that needs to be added to the block chain",
            "required": true,
            "schema": {
              "$ref": "#/definitions/Certificate"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Result"
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "404": {
            "description": "Not found"
          },
          "405": {
            "description": "Validation exception"
          }
        }
      }
    },
    "/certificate/{certId}": {
      "get": {
        "tags": [
          "certificate"
        ],
        "summary": "Finds Certificate by CertId",
        "produces": [
          "application/xml",
          "application/json"
        ],
        "parameters": [
          {
            "name": "certId",
            "in": "path",
            "type": "string",
            "required": true
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Certificate"
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "404": {
            "description": "Cert not found"
          }
        }
      }
    },
    "/openstatus": {
      "post": {
        "tags": [
          "certificate"
        ],
        "consumes": [
          "application/json",
          "application/xml"
        ],
        "produces": [
          "application/xml",
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "certId",
            "required": true,
            "type": "string"
          },
          {
            "in": "query",
            "name": "isOpen",
            "required": true,
            "type": "boolean"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Result"
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "404": {
            "description": "Not found"
          },
          "405": {
            "description": "Validation exception"
          }
        }
      }
    },
    "/certificate/openlist": {
      "get": {
        "tags": [
          "certificate"
        ],
        "produces": [
          "application/xml",
          "application/json"
        ],
        "parameters": [
          {
            "name": "startDate",
            "in": "query",
            "description": "校准日期起始日期",
            "type": "integer",
            "required": false
          },
          {
            "name": "endDate",
            "in": "query",
            "description": "校准日期结束日期",
            "required": false,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Certificate"
              }
            }
          },
          "400": {
            "description": "Invalid input"
          },
          "404": {
            "description": "Certs not found"
          }
        }
      }
    },
    "/certificate/unfinishedlist": {
      "get": {
        "tags": [
          "certificate"
        ],
        "produces": [
          "application/xml",
          "application/json"
        ],
        "parameters": [
          {
            "name": "startDate",
            "in": "query",
            "description": "校准日期起始日期",
            "type": "integer",
            "required": false
          },
          {
            "name": "endDate",
            "in": "query",
            "description": "校准日期结束日期",
            "required": false,
            "type": "integer"
